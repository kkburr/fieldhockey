<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Usfha number</th>
      <th>School</th>
      <th>Grade</th>
      <th>Birthday</th>
      <th>Allergies</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @players.each do |player| %>
      <tr>
        <td><%= player.first_name %></td>
        <td><%= player.last_name %></td>
        <td><%= player.usfha_number %></td>
        <td><%= player.school %></td>
        <td><%= player.grade %></td>
        <td><%= player.birthday %></td>
        <td><%= player.allergies %></td>
        <!-- <td><%= link_to 'Add Program', new_program_path %></td> -->
        <td><%= link_to 'Edit', edit_player_path(player) %></td>
        <td><%= link_to 'Destroy', player, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>